#include <stdio.h>
#include <conio.h>

main()
{   int i,B[3][3],  A[3][3],j;
    printf("enter the number of matrix");
    for(i=0;i<=2;i++)
    {
        for (j=0;j<=2;j++)
            scanf("%d",&A[i][j]);
    }


    for(i=0;i<=2;i++)
    {
        for (j=0;j<=2;j++)
            scanf("%d",&B[i][j]);
    }



    for(i=0;i<=2;i++)
    {
        for (j=0;j<=2;j++)
        printf("%d\n",A[i][j]+B[i][j])      ;
    }


}
